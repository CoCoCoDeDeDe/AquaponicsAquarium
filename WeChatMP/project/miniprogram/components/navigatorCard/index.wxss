.navigatorCardBox {
  padding: 10px 15px;
}

.navigatorCard {
  padding: 17px 18px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;

  border-radius: 20px;
  background-color: #fefefe;
  box-shadow: 2px 2px 8px 1px rgba(0, 0, 0, 0.03);
}
.navigatorIconBox {
  display: flex;
}
.navigatorIcon {
  --edge-length: 34px;
  height: var(--edge-length);
  width: var(--edge-length);
}
.navigatorTextBox {
  padding-left: 15px;
  display: flex;
  align-items: center;
  height: 38px;
}
.navigatorText {
  display: inline-block;
  font-size: 19px;
}